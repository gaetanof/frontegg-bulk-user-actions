# Frontegg API Configuration
FRONTEGG_CLIENT_ID=env_client_id
FRONTEGG_API_TOKEN=env_api_token
FRONTEGG_TENANT_ID=
FRONTEGG_REGION=EU  # Options: US, EU, AP

# Rate Limiting Configuration
RATE_LIMIT_DELAY=0.5  # Seconds between API calls
MAX_RETRIES=3
PAGE_SIZE=200

# Deletion Configuration
# Comma-separated list of allowed email domains
# Users with email domains NOT in this list will be deleted
USER_ID_ARRAY=test@test.com,test@test.com
USER_ACTION=   # Options: lock, delete
